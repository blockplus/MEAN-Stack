{
  "extends": "airbnb",
  "globals": {
    "__DEV__": true,
    "StripeCheckout": true,
    "google": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": 0,
    "max-len": 0,
    "no-else-return": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-quotes": 0,
    "react/jsx-no-bind": 0,
    "jsx-quotes": [2, "prefer-double"]
  }
}
